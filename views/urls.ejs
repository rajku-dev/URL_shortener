<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>All Registered URL</h1>
    <% if (locals.allURL) { %>
        <table>
            <thead>
                <th>S.NO</th>
                <th>Redirected URL</th>
                <th>Shortened URL</th>
                <th>Total Clicks</th>
                <th>Click to Go</th>
            </thead>
            <tbody>
                <% allURL.forEach((url,index) => { %>
                 <tr>
                    <td><%= index+1 %></td>
                    <td><%= url.redirectURL %></td>
                    <td><%= url.shortID %></td>
                    <td><%= url.totalClicks %></td>
                    <td><a href="http://localhost:3000/url/<%= url.shortID %>">Visit <%= url.redirectURL.split('.')[1] %></a></td>
                 </tr>
                <% }) %>
            </tbody>
        </table>
    <% } %>
</body>
</html>